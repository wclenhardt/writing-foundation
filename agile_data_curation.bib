  pages = {69--82},
  file = {EBSCO Full Text:/Users/kbene/Library/Application Support/Zotero/Profiles/osd26ql5.default/zotero/storage/9QQTXW7P/Hamel - 1996 - STRATEGY AS REVOLUTION. (cover story).pdf:application/pdf},
