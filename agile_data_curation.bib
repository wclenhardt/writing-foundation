  month = feb,
  year = {2011},
  keywords = {alcohol gel,epidemiology,NVIVO Export,observational study,positive deviance strategy,Step-down unit,sustainability},
  pages = {1--5},
  file = {ScienceDirect Full Text PDF:/Users/kbene/Library/Application Support/Zotero/Profiles/osd26ql5.default/zotero/storage/TZNXCJ8T/Marra et al. - 2011 - Positive deviance A program for sustained improve.pdf:application/pdf;ScienceDirect Snapshot:/Users/kbene/Library/Application Support/Zotero/Profiles/osd26ql5.default/zotero/storage/EERMA8M3/S0196655310007388.html:text/html},
